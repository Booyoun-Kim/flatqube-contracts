PUSHINT 1073820897
LEQ
PUSHCONT {
	DUP
	PUSHINT 744553903
	EQUAL
	IFJMPREF {
		CALL $notifyTokenRootDeployed$
	}
	DUP
	PUSHINT 1073820897
	EQUAL
	IFJMPREF {
		CALL $notifyTokenRootNotDeployed$
	}
}
IFJMP
DUP
PUSHINT 1354331633
LEQ
PUSHCONT {
	DUP
	PUSHINT 1196840156
	EQUAL
	IFJMPREF {
		CALL $constructor$
	}
	DUP
	PUSHINT 1354331633
	EQUAL
	IFJMPREF {
		CALL $getDeployedToken$
	}
}
IFJMP
DUP
PUSHINT 2054428688
EQUAL
IFJMPREF {
