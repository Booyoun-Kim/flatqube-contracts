.macro constructor
DROP
GETGLOB 1
ISNULL
IFREF {
	CALL $c4_to_c7_with_init_storage$
}
;; constructor protection
GETGLOB 6
THROWIF 51
PUSHINT 1
SETGLOB 6
;; end constructor protection
; Decode input parameters
; Decode factory
LDMSGADDRQ
PUSHCONT {
	;; load next cell
	LDREF
	ENDS
	CTOS
	LDMSGADDR
}
IFNOT
ENDS
;; param: factory
; constructor TokenFactoryCreateNewTokenFor
ACCEPT
; expValue
; end expValue
;; push identifier factory
DUP
; colValue
SETGLOB 11
; end colValue
; end constructor TokenFactoryCreateNewTokenFor
DROP
CALLREF {
	CALL $c7_to_c4$
}
TRUE
SETGLOB 7

.macro newToken
DROP
GETGLOB 1
ISNULL
IFREF {
	CALL $c4_to_c7$
}
; Decode input parameters
; Decode answer_id
LDU 32
; Decode value
LDUQ 128
PUSHCONT {
	;; load next cell
	LDREF
	ENDS
	CTOS
	LDU 128
}
IFNOT
; Decode owner
LDMSGADDRQ
PUSHCONT {
	;; load next cell
	LDREF
	ENDS
	CTOS
	LDMSGADDR
}
IFNOT
; Decode name
DUP
SREFS
EQINT 1
PUSHCONT {
	LDREF
	ENDS
	CTOS
}
IF
LDREF
; Decode symbol
DUP
SBITREFS
EQINT 1
SWAP
EQINT 0
AND
PUSHCONT {
	LDREF
	ENDS
	CTOS
}
IF
LDREF
; Decode decimals
LDUQ 8
PUSHCONT {
	;; load next cell
	LDREF
	ENDS
	CTOS
	LDU 8
}
IFNOT
ENDS
;; param: answer_id
;; param: value
;; param: owner
;; param: name
;; param: symbol
;; param: decimals
CALLREF {
	CALL $newToken_internal_macro$
}
IFREF {
	CALL $c7_to_c4$
}
TRUE
SETGLOB 7

.globl	newToken_internal
.type	newToken_internal, @function
CALL $newToken_internal_macro$

.macro newToken_internal_macro
;; param: answer_id
;; param: value
;; param: owner
;; param: name
;; param: symbol
;; param: decimals
; function newToken
ACCEPT
;; Remote call ITokenFactory.Token
;; push identifier value
PUSH S4
;; push identifier _factory
GETGLOB 11
NEWC
STSLICECONST x62_
STSLICE
SWAP
STGRAMS
PUSHINT 105
STZEROES
STSLICECONST 0
STSLICECONST 1
NEWC
STSLICECONST x64be7ee7
;; answer_id
;; push identifier answer_id
PUSH S7
STUR 32
;; root_public_key
PUSHINT 0
STUR 256
;; root_owner_address
;; push identifier owner
PUSH S5
STSLICER
;; name
;; push identifier name
PUSH S4
STREFR
;; symbol
;; push identifier symbol
PUSH S3
STREFR
;; decimals
;; push identifier decimals
PUSH S2
STUR 8
STBREFR
ENDC
PUSHINT 0
SENDRAWMSG
; end function newToken
BLKDROP 6

